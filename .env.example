# Environment variables for the DBT Skills Learning App

# Database
DATABASE_URL="postgresql://username:password@host:port/database"

# Clerk Authentication Keys (Required)
# Standard format
CLERK_PUBLISHABLE_KEY="your_publishable_key_here"
CLERK_SECRET_KEY="your_secret_key_here"

# Nuxt-specific format (also required)
NUXT_PUBLIC_CLERK_PUBLISHABLE_KEY="your_publishable_key_here"
NUXT_CLERK_SECRET_KEY="your_secret_key_here"
